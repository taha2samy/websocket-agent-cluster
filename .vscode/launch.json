{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django: Makemigrations",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["makemigrations"],
            "console": "integratedTerminal",
        },
        {
            "name": "Django: Migrate",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["migrate"],
            "console": "integratedTerminal",
        },
        {
            "name": "Run Uvicorn ASGI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["runserver", "0.0.0.0:8000"],
            "console": "integratedTerminal",
        },
        {
            "name": "Django: Create Superuser",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["createsuperuser"],
            "console": "integratedTerminal",
        },
                {
            "name": "Client: tkinter client",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/client/tkinter/main.py",
            "console": "integratedTerminal",
            "python": "/home/Taha/.pyenv/shims/python"
        },
        {
            "name": "Run Daphne ASGI",
            "type": "python",
            "request": "launch",
            "module": "daphne",
            "args": [
                "-b", "0.0.0.0",
                "-p", "8000",
                "myproject.asgi:application"
            ],
            "env": {
                "DJANGO_SETTINGS_MODULE": "myproject.settings"
            },
            "console": "integratedTerminal"
        }


    ]
}
